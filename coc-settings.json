{
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.enableLinkedEditing": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "coc.source.file.ignoreHidden": false,
  "diagnostic.errorSign": "󰅚 ",
  "diagnostic.warningSign": "󰀪 ",
  "diagnostic.infoSign": "󰋽 ",
  "diagnostic.hintSign": "󰌶 ",
  "explorer.file.reveal.auto": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.root.customRules": {
    "vcs": {
      "patterns": [
        ".git",
        ".hg",
        ".projections.json"
      ]
    },
    "vcs-r": {
      "patterns": [
        ".git",
        ".hg",
        ".projections.json"
      ],
      "bottomUp": true
    }
  },
  "explorer.root.strategies": [
    "custom:vcs",
    "custom:vcs-r",
    "workspace",
    "cwd",
    "sourceBuffer",
    "reveal"
  ],
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.width": 30,
  "list.source.grep.useLiteral": false,
  "list.source.grep.defaultArgs": [
    "--ignore-case"
  ],
  "session.directory": "~/.config/nvim/sessions",
  "session.saveOnVimLeave": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.maxCompleteItemCount": 200,
  "yank.highlight.duration": 200,
  "java.jdt.ls.vmargs": "-javaagent:/Users/zhanghf/.config/nvim/lombok.jar",
  "java.jdt.ls.java.home": "/usr/local/opt/openjdk@17",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/usr/local/opt/openjdk@8"
    },
    {
      "name": "JavaSE-11",
      "path": "/usr/local/opt/openjdk@11"
    },
    {
      "name": "JavaSE-17",
      "path": "/usr/local/opt/openjdk@17",
      "default": true
    }
  ],
  "java.completion.maxResults": 200
}
