{
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.enableLinkedEditing": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "go",
    "python",
    "rust",
    "markdown",
    "javascript",
    "typescript",
    "java",
    "json",
    "jsonc"
  ],
  "coc.source.file.ignoreHidden": false,
  "list.source.grep.useLiteral": false,
  "list.source.grep.defaultArgs": ["--ignore-case"],
  "session.directory": "~/.config/nvim/sessions",
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.maxCompleteItemCount": 200,
  "yank.highlight.duration": 200,
  "java.jdt.ls.vmargs": "-javaagent:/Users/zhanghf/.vim/lombok.jar",
  "java.home": "/usr/local/opt/openjdk@17",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/usr/local/opt/openjdk@8"
    },
    {
      "name": "JavaSE-11",
      "path": "/usr/local/opt/openjdk@11"
    },
    {
      "name": "JavaSE-17",
      "path": "/usr/local/opt/openjdk@17",
      "default": true
    }
  ],
  "java.completion.maxResults": 200
}
