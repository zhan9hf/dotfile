{
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.enableLinkedEditing": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "coc.source.file.ignoreHidden": false,
  "copilot.enablePreselect": false,
  "copilot.limit": 10,
  "copilot.priority": 50,
  "diagnostic.errorSign": "󰅚 ",
  "diagnostic.warningSign": "󰀪 ",
  "diagnostic.infoSign": "󰋽 ",
  "diagnostic.hintSign": "󰌶 ",
  "diagnostic-languageserver.filetypes": {
    "cpp": [
      "cppcheck"
    ],
    "c": [
      "cppcheck"
    ]
  },
  "explorer.file.reveal.auto": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.root.customRules": {
    "vcs": {
      "patterns": [
        ".git",
        ".hg",
      ]
    },
    "vcs-r": {
      "patterns": [
        ".git",
        ".hg",
      ],
      "bottomUp": true
    }
  },
  "explorer.root.strategies": [
    "custom:vcs",
    "custom:vcs-r",
    "workspace",
    "cwd",
    "sourceBuffer",
    "reveal"
  ],
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.width": 30,
  "git.addGBlameToBufferVar": true,
  "git.changedDecorator": " 󰃙",
  "git.stagedDecorator": " 󰃘",
  "git.untrackedDecorator": " ✩",
  "git.conflictedDecorator": "⇡⇣",
  "git.virtualTextPrefix": "  ",
  "list.source.diagnostics.includeCode": false,
  "list.source.diagnostics.pathFormat": "short",
  "list.source.grep.useLiteral": false,
  "list.source.grep.defaultArgs": [
    "--ignore-case"
  ],
  "snippets.userSnippetsDirectory": "~/UltiSnips",
  "snippets.extends": {
    "cpp": [
      "c"
    ],
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "suggest.enablePreselect": false,
  "suggest.labelMaxLength": 50,
  "suggest.maxCompleteItemCount": 200,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": true,
  "suggest.selection": "recentlyUsedByPrefix",
  "suggest.virtualText": true,
  "yank.highlight.duration": 200,
  "java.jdt.ls.vmargs": "-javaagent:/Users/zhanghf/.config/nvim/lombok.jar",
  "java.jdt.ls.java.home": "/usr/local/opt/openjdk@17",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/usr/local/opt/openjdk@8"
    },
    {
      "name": "JavaSE-11",
      "path": "/usr/local/opt/openjdk@11"
    },
    {
      "name": "JavaSE-17",
      "path": "/usr/local/opt/openjdk@17",
      "default": true
    }
  ],
  "java.completion.maxResults": 200
}
